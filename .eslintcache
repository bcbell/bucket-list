idth  >  580)  {\n\t\tsetNumberOfMonth(2);\n\t}  else  if  (width  <  580)  {\n\t\tsetNumberOfMonth(2);\n\t}\n};\n\nconst today = Date.now()\n\nconst  Day = ({  day  }) => {\n\treturn  (\n\t\t<>\n\t\t\t<p  className=\"date\">{day.format('DD')}</p>\n\t\t\t{/* <p  className=\"date\"></p> */}\n\t\t</>\n\t\t);\n\t};\nconst  onChange = date => console.log(date)\t\n\nclass Calendar extends Component {\n\n  render() { \n       \n    return (\n      <ThemeProvider theme={theme}>\n        <RangePicker\n          handleChange={onChange}\n\t\t  selectedDays={[today]} //initial selected days\n\t\t  jalali={false}\n\t\t  numberOfMonths={1}\n\t\t  numberOfSelectableDays={30} // number of days you need \n\t\t  disabledDays={['2019-12-02']} //disabeld days\n\t\t  responsive={handleResponsive} // custom responsive, when using it, `numberOfMonths` props not working\n\t\t  disabledBeforToday={true} \n\t\t  hoverable= {true}\n\t\t  disabled={false} // disable calendar \n\t\t  dayComponent={Day} //custom day component \n\t\t  titleComponent='Title' // custom title of days {Title}\n        />\n      </ThemeProvider>\n    );\n  }\n}\n\n\nexport default Calendar","/Users/brittanycoleman/code/react/project/bucket-list/src/components/POICard/POICard.jsx",[],"/Users/brittanycoleman/code/react/project/bucket-list/src/components/Counter/Countdown.jsx",["272"],"/Users/brittanycoleman/code/react/project/bucket-list/src/components/SearchForm/SearchForm.jsx",[],"/Users/brittanycoleman/code/react/project/bucket-list/src/components/CardProfile/CardProfile.jsx",["273","274","275","276","277","278","279"],"import React from 'react';\nimport './CardProfile.css'\n\nconst ImgUpload =({\n  onChange,\n  src\n})=>\n  <label htmlFor=\"photo-upload\" className=\"custom-file-upload fas\">\n    <div className=\"img-wrap img-upload\" >\n      <img className='userProfile' for=\"photo-upload\" src={src}/>\n    </div>\n    <br/>\n     <input className='chooseFile' id=\"photo-upload\" type=\"file\" onChange={onChange}/> \n    \n  </label>\n\n\nconst Name =({\n  onChange,\n  value\n})=>\n  <div className=\"field\">\n    <label className= 'cardProfile' htmlFor=\"name\">\n      name:\n    </label>\n    <input \n      id=\"name\" \n      type=\"text\" \n      onChange={onChange} \n      maxlength=\"25\" \n      value={value} \n      placeholder=\"Alexa\" \n      required/>\n  </div>\n\n  \nconst Status =({\n  onChange,\n  value\n})=>\n  <div className=\"field\">\n    <label className='cardProfile' htmlFor=\"status\">\n      status:\n    </label>\n    <input \n      id=\"status\" \n      type=\"text\" \n      onChange={onChange} \n      maxLength=\"35\" \n      value={value} \n      placeholder=\"It's a nice day!\" \n      required/>\n  </div>\n\nconst Dream =({\n  onChange,\n  value\n})=>\n  <div className=\"field\">\n    <label className='cardProfile' htmlFor=\"dream\">\n      dream destination:\n    </label>\n    <input \n      id=\"dream\" \n      type=\"text\" \n      onChange={onChange} \n      maxLength=\"35\" \n      value={value} \n      placeholder=\"Bora Bora!\" \n      required/>\n  </div>\n\nconst Favorite =({\n  onChange,\n  value\n})=>\n  <div className=\"field\">\n    <label className='cardProfile' htmlFor=\"favorite\">\n      favorite destination:\n    </label>\n    <input \n      id=\"favorite\" \n      type=\"text\" \n      onChange={onChange} \n      maxLength=\"35\" \n      value={value} \n      placeholder=\"Africa!\" \n      required/>\n  </div>\nconst Profile =({\n  onSubmit,\n  src,\n  name,\n  status,\n  dream,\n  favorite\n})=>\n  <div className=\"profilePic\">\n    <form className='cardProfile'onSubmit={onSubmit}>\n      <label className=\"custom-file-upload fas\">\n        <div className=\"img-wrap\" >\n          <img className='userProfile' for=\"photo-upload\" src={src}/>\n        </div>\n      </label>\n      <div className='inputProfile'>\n        <div className=\"name\">{name}</div>\n        <div className=\"status\">{status}</div>\n        <div className='dream'>{dream}</div>\n        <div className='favorite'>{favorite}</div>\n      <button type=\"submit\" className=\"edit\">Edit Profile </button>\n      </div>\n    </form>\n  </div>\n     \n      \nconst Edit =({\n  onSubmit,\n  children,\n})=>\n  <div className=\"profilePic\">\n    <form onSubmit={onSubmit}>\n        {children}\n      <button type=\"submit\" className=\"save\">Save </button>\n    </form>\n  </div>\n\nclass CardProfile extends React.Component {\n  state = {\n    file: '',\n    imagePreviewUrl: 'https://github.com/OlgaKoplik/CodePen/blob/master/profile.jpg?raw=true',\n    name:'',\n    status:'',\n    dream: '',\n    favorite: '',\n    active: 'edit'\n  }\n\n  photoUpload = e =>{\n    e.preventDefault();\n    const reader = new FileReader();\n    const file = e.target.files[0];\n    reader.onloadend = () => {\n      this.setState({\n        file: file,\n        imagePreviewUrl: reader.result\n      });\n    }\n    reader.readAsDataURL(file);\n  }\n  editName = e =>{\n    const name = e.target.value;\n    this.setState({\n      name,\n    });\n  }\n  \n  editStatus = e => {\n    const status = e.target.value;\n    this.setState({\n      status,\n    });\n  }\n  \n  handleSubmit= e =>{\n    e.preventDefault();\n    let activeP = this.state.active === 'edit' ? 'profile' : 'edit';\n    this.setState({\n      active: activeP,\n    })\n  }\n  \n  render() {\n    const {imagePreviewUrl, \n           name, \n           status, \n           dream,\n           favorite,\n           active} = this.state;\n    return (\n      <div>\n\n       \n        {(active === 'edit')?(\n          <Edit onSubmit={this.handleSubmit}>\n            <ImgUpload onChange={this.photoUpload} src={imagePreviewUrl}/>\n            <Name onChange={this.editName} value={name}/>\n            <Status onChange={this.editStatus} value={status}/>\n            <Dream onChange={this.editDream} value={dream}/>\n            <Favorite onChange={this.editFavorite} value={favorite}/>\n     