r})=> {\n\n\n    return (\n        <div>\n            <>\n            <div className='viewTrips'>\n                <h1  className='viewTrips'> Bucket List</h1>\n                <div className='trips'>\n                    <div className='planner'>\n                      <Countdown/>\n                    </div>\n                    <div className='itineraryTrips'>\n                    <h2 key={user.id}className='viewTrips'>Upcoming & Future Trips for {user.name} </h2>\n                       <ItineraryCards /> \n                    </div>\n                    \n                </div>\n                \n                </div>\n                \n            </>\n        </div>\n    )\n}\n\n\n\nexport default ViewTrips\n","/Users/sophiaseremetis/Documents/code/unit-3-project/bucket-list/src/pages/Trips/AddTrips/AddTrips.jsx",[],"/Users/sophiaseremetis/Documents/code/unit-3-project/bucket-list/src/components/Buttons/FooterButtons/FooterButtons.jsx",["259"],"import React from 'react'\nimport './FooterButtons.css'\n\nimport ForumIcon from '@material-ui/icons/Forum';\nimport AccountCircleIcon from '@material-ui/icons/AccountCircle';\nimport FavoriteIcon from '@material-ui/icons/Favorite'\nimport ChatIcon from '@material-ui/icons/Chat';\nimport IconButton from \"@material-ui/core/IconButton\"\nimport {Link} from 'react-router-dom'\n\n\nfunction FooterButtons() {\n    return (\n        <div className='swipeButtons'>\n            <a href='/profile'>\n                <IconButton className='swipeButtons_left'>\n                    <AccountCircleIcon fontSize='large'/>\n                </IconButton>\n            </a>\n            <a href='/discussion'>\n                <IconButton className='swipeButtons_star'>\n                    <ForumIcon fontSize='large'/>\n                </IconButton>\n            </a>\n            <a href='/favorites'>\n            <IconButton className='swipeButtons_right'>\n                <FavoriteIcon fontSize='large'/>\n            </IconButton>\n   