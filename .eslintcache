am={dream}\n            favorite={favorite}/>)}\n        \n      </div>\n    )\n  }\n}\n\nexport default CardProfile",{"ruleId":"142","replacedBy":"143"},{"ruleId":"144","replacedBy":"145"},{"ruleId":"146","severity":1,"message":"147","line":4,"column":8,"nodeType":"148","messageId":"149","endLine":4,"endColumn":16},{"ruleId":"146","severity":1,"message":"150","line":7,"column":26,"nodeType":"148","messageId":"149","endLine":7,"endColumn":34},{"ruleId":"146","severity":1,"message":"151","line":52,"column":7,"nodeType":"148","messageId":"149","endLine":52,"endColumn":11},{"ruleId":"146","severity":1,"message":"152","line":2,"column":10,"nodeType":"148","messageId":"149","endLine":2,"endColumn":20},{"ruleId":"153","severity":1,"message":"154","line":89,"column":30,"nodeType":"148","endLine":89,"endColumn":37},{"ruleId":"146","severity":1,"message":"155","line":2,"column":19,"nodeType":"148","messageId":"149","endLine":2,"endColumn":32},{"ruleId":"146","severity":1,"message":"156","line":2,"column":46,"nodeType":"148","messageId":"149","endLine":2,"endColumn":54},{"ruleId":"146","severity":1,"message":"157","line":9,"column":15,"nodeType":"148","messageId":"149","endLine":9,"endColumn":23},{"ruleId":"146","severity":1,"message":"158","line":10,"column":13,"nodeType":"148","messageId":"149","endLine":10,"endColumn":15},{"ruleId":"159","severity":1,"message":"160","line":9,"column":7,"nodeType":"161","endLine":9,"endColumn":66},{"ruleId":"159","severity":1,"message":"160","line":101,"column":11,"nodeType":"161","endLine":101,"endColumn":70},"no-native-reassign",["162"],"no-negated-in-lhs",["163"],"no-unused-vars","'Calendar' is defined but never used.","Identifier","unusedVar","'setState' is assigned a value but never used.","'path' is assigned a value but never used.","'DatePicker' is defined but never used.","react-hooks/exhaustive-deps","The ref value 'interval.current' will likely have changed by the time this effect cleanup function runs. If this ref points to a nod